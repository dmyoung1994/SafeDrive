MojioModel = require('./MojioModel')

module.exports = class {{Model}} extends MojioModel
    # instance variables
    _schema: {{schema}}

    _resource: '{{Resource}}'
    _model: '{{Model}}'

    constructor: (json) ->
        super(json)

    # class variables and functions
    @_resource: '{{Resource}}'
    @_model: '{{Model}}'

    @resource: () ->
        return {{Model}}._resource

    @model: () ->
        return {{Model}}._model

